{
  "name": "react-starter",
  "version": "3.5.0",
  "description": "Koa react",
  "private": false,
  "main": "src/server/app.js",
  "author": "Paralect",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paralect/koa-react-starter.git"
  },
  "keywords": [
    "koa",
    "react",
    "redux",
    "paralect"
  ],
  "engines": {
    "node": ">= 8"
  },
  "scripts": {
    "build-client": "webpack -p --config src/client/webpack.config.js",
    "start": "node src/server/app.js",
    "test": "run-s test:**",
    "test:server": "eslint ./src/server",
    "test:client": "eslint --ext js --ext jsx ./src/client",
    "test:lint-styles": "stylelint \"src/client/**/*.pcss\"",
    "development": "nodemon --watch src/server --watch src/client/webpack.dev.config.js src/server/app.js",
    "add-contributor": "all-contributors add",
    "generate-contributors": "all-contributors generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ext js --ext jsx --fix"
    ],
    "*.pcss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@koa/router": "9.0.1",
    "@paralect/common-logger": "0.1.10",
    "app-module-path": "2.2.0",
    "axios": "0.19.2",
    "classnames": "2.2.6",
    "connected-react-router": "6.8.0",
    "core-js": "3.6.5",
    "handlebars": "4.7.6",
    "history": "4.10.1",
    "incstr": "1.2.3",
    "koa": "2.13.0",
    "koa-logger": "3.2.1",
    "koa-mount": "4.0.0",
    "koa-static": "5.0.0",
    "koa-views": "6.3.0",
    "koa-webpack": "5.3.0",
    "lodash": "4.17.19",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.21",
    "react-icon-base": "2.1.2",
    "react-icons": "3.10.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.5",
    "socket.io-client": "2.3.0",
    "uuid": "8.1.0",
    "webpack": "4.43.0",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.3",
    "@babel/core": "7.10.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@hot-loader/react-dom": "16.13.0",
    "all-contributors-cli": "6.16.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-inline-react-svg": "1.1.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-react-css-modules": "5.2.6",
    "css-loader": "3.6.0",
    "cssnano": "4.1.10",
    "eslint": "7.3.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "lodash-webpack-plugin": "0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "nodemon": "2.0.4",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-standard": "20.0.0",
    "url-loader": "4.1.0",
    "webpack-cli": "3.3.12"
  }
}
